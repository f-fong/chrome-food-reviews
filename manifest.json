{
    "name": "Food reviews",
    "description": "Get those food reviews!",
    "version": "0.1",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://www.ubereats.com/"
    ],
    "content_scripts": [{
        "matches": ["https://www.ubereats.com/*"],
        "js": ["content-script.js"]
    }],
    "web_accessible_resources": ["script.js"],
    "manifest_version": 2
}
