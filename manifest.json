{
    "name": "Food reviews",
    "description": "Get those food reviews!",
    "version": "0.1",
    "browser_action": {
        "default_popup": "build/popup.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "https://www.ubereats.com/",
        "https://api.yelp.com/"
    ],
    "content_scripts": [{
        "matches": ["https://www.ubereats.com/*"],
        "js": ["build/load.js"]
    }],
    "web_accessible_resources": ["build/main.js"],
    "manifest_version": 2
}
